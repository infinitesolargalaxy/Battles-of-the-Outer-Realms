		0xD3090B** Stations
		0xD3090C** Items
		0xD3090D** Missions
		0xD3090E** Systems + Sovereigns + Dockscreens
		-->
		
	<!ENTITY unidExtension						"0xD3090000">
	
	<!-- Ships -->
	<!ENTITY scBritannia2						"0xD3090A00">
	<!ENTITY scBritannia2Mark					"0xD3090A01"> 
	<!ENTITY scAurochsFrigate					"0xD3090A02">
	<!ENTITY scPolarCarrier						"0xD3090A03">
	<!ENTITY scChasm2							"0xD3090A04">
	<!ENTITY scPhobos2Prototype					"0xD3090A05">
	<!ENTITY scChasm2AEGIS3B					"0xD3090A06">
	<!ENTITY scAquilaCruiser2					"0xD3090A07">
	<!ENTITY scBritanniaM						"0xD3090A08">
	<!ENTITY scSandstorm2						"0xD3090A09">
	<!ENTITY scCSCAmerica						"0xD3090A0A">
	<!ENTITY scCSCIndia							"0xD3090A0B">
	<!ENTITY scCSCAtlantica						"0xD3090A0C">
	<!ENTITY scCSCPacifica						"0xD3090A0D">
	<!ENTITY scCSCAsia							"0xD3090A0E">
	
	<!-- Stations -->
	<!ENTITY stCastorStargate					"0xD3090B00">
	<!ENTITY stPointMinerva						"0xD3090B01">
	<!ENTITY stCommonFleetTurret				"0xD3090B02">
	<!ENTITY stOmegaStation						"0xD3090B03">
	
	<!-- Items -->
	<!ENTITY vtICXTev9							"0xD3090C00">
	<!ENTITY itTeV9PD							"0xD3090C01">	
	<!ENTITY itAresSpear						"0xD3090C02">
	<!ENTITY itAresROM							"0xD3090C03">
	<!ENTITY itCargoCrates						"0xD3090C04">
	<!ENTITY itR13Deflector						"0xD3090C05">
	
	
	<!-- Missions -->
	<!ENTITY msWeakenAresFoothold				"0xD3090D00">
	<!ENTITY msInvestigateColony				"0xD3090D01">
	<!ENTITY msBlockadeRunning					"0xD3090D02">
	<!ENTITY msInterceptAresVanguardFleet		"0xD3090D03">
	<!ENTITY msFortify							"0xD3090D04">
	<!ENTITY msInterceptAresMainFleet			"0xD3090D05">
	<!ENTITY msRaidFreighter					"0xD3090D06">
	<!ENTITY msRendezvous						"0xD3090D07">
		
	<!-- Systems -->	
	<!ENTITY unidDenebSegment					"0xD3090E00">
	<!ENTITY unidTopology						"0xD3090E01">
	<!ENTITY ssDeneb							"0xD3090E02">
	
	
]>
<TranscendenceExtension UNID="0xD3090000;" apiVersion="28" version="1.1" name="Battles of the Outer Realms" credits="SolarGalaxy">

	<!-- Libraries -->
	<Library unid="&unidHumanSpaceLibrary;"/>
	
<Modules>
		<Module filename="BoTOR_CWFleet.xml"/>
		<Module filename="BoTOR_Ares.xml"/>
		<Module filename="BoTOR_Minerva.xml"/>		
</Modules>

<!-- This grafts the Deneb segment onto the main line -->
	
	<SystemMap UNID="&unidTopology;"
			displayOn="&smHumanSpace;"
			>

		<Uses unid="&unidPartISystemMap;"/>
		<Uses unid="&unidDenebSegment;"/>

		<TopologyCreator>
			<Node ID="DenebSegment"/>
		</TopologyCreator>
	</SystemMap>
	
	<!--CEPHEI SEGMENT ================================================ -->

	<SystemMap UNID="&unidDenebSegment;"
			displayOn="&smHumanSpace;"
			>

		<NodeGroup ID="DenebSegment">
		<!-- Connected to Dantalion -->
			<Stargate from="A7:Edgeward"			to="Deneb:Coreward"/>
		</NodeGroup>
		
		<!-- Deneb -->
		
		<Node ID="Deneb" x="-400" y="-281">
			<System 
					name=			"Deneb"
					level=			"10"
					attributes=		"humanSpace, outerRealm"
					>
				<System UNID="&ssDeneb;"/>
			</System>
		</Node>
		
	</SystemMap>
	
	

</TranscendenceExtension>